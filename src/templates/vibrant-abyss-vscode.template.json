    "editorGroup.border": "{{background_primary}}",
    "editorGroup.dropBackground": "{{background_primary}}",
    "editorGroup.emptyBackground": "{{background_primary}}",
    "editorGroupHeader.noTabsBackground": "{{background_primary}}",
    "editorGroupHeader.tabsBackground": "{{background_primary}}",
    "editorGroupHeader.tabsBorder": "{{background_primary}}",
    "editorHoverWidget.background": "{{background_primary}}",
    "editorHoverWidget.border": "{{background_primary}}",
    "editorIndentGuide.activeBackground1": "{{background_primary}}",
