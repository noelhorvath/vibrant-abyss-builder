    "editorSuggestWidget.highlightForeground": "{{syntax_keyword}}",
    "editorSuggestWidget.selectedBackground": "{{background_transparent}}",
    "editorRuler.foreground": "{{border_dim}}",
    "editorWarning.foreground": "{{background_primary}}",
    "editorWidget.background": "{{background_primary}}",
    "editorWidget.border": "{{background_primary}}",
    "input.background": "{{background_primary}}",
    "input.placeholderForeground": "{{foreground_secondary}}",
    "list.activeSelectionIconForeground": "{{foreground_primary}}",
    "list.activeSelectionForeground": "{{foreground_primary}}",
    "list.dropBackground": "{{background_primary}}",
    "list.errorForeground": "{{state_error}}",
    "list.warningForeground": "{{state_warning}}",
    "menu.background": "{{background_primary}}",
    "menu.foreground": "{{foreground_secondary}}",
    "notificationCenterHeader.background": "{{background_primary}}",
    "notificationLink.foreground": "{{state_info}}",
    "notifications.background": "{{background_primary}}",
    "notifications.border": "{{background_primary}}",
